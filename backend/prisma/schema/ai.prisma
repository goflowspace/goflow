// AI —Å–∏—Å—Ç–µ–º–∞ –¥–ª—è –ø—Ä–æ–∞–∫—Ç–∏–≤–Ω—ã—Ö –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–π

// –ú–æ–¥–µ–ª—å –¥–ª—è —Ç—Ä–µ–∫–∏–Ω–≥–∞ AI –∑–∞–ø—Ä–æ—Å–æ–≤
model AIRequest {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  userId        String   @db.ObjectId
  projectId     String   @db.ObjectId
  type          AIRequestType
  
  // –ö–æ–Ω—Ç–µ–∫—Å—Ç –∑–∞–ø—Ä–æ—Å–∞
  context       Json     // { nodeId, surroundingNodes, projectMeta }
  
  // –ü—Ä–æ–≤–∞–π–¥–µ—Ä –∏ –º–µ—Ç—Ä–∏–∫–∏
  provider      AIProvider
  tokensUsed    Int      @default(0)
  creditsCharged Int     @default(0)
  responseTime  Int?     // milliseconds
  
  // –°—Ç–∞—Ç—É—Å –∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç
  status        AIRequestStatus @default(PENDING)
  result        Json?    // –†–µ–∑—É–ª—å—Ç–∞—Ç –∑–∞–ø—Ä–æ—Å–∞
  errorMessage  String?  // –°–æ–æ–±—â–µ–Ω–∏–µ –æ–± –æ—à–∏–±–∫–µ
  
  // –°–≤—è–∑–∏
  user          User     @relation("UserAIRequests", fields: [userId], references: [id], onDelete: Cascade)
  project       Project  @relation("ProjectAIRequests", fields: [projectId], references: [id], onDelete: Cascade)
  suggestions   AISuggestion[]
  
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  
  @@map("ai_requests")
  @@index([userId, createdAt])
  @@index([projectId, createdAt])
  @@index([status, createdAt])
}

// –ú–æ–¥–µ–ª—å –¥–ª—è AI –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–π
model AISuggestion {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  requestId     String   @db.ObjectId
  userId        String   @db.ObjectId
  projectId     String   @db.ObjectId
  
  // –ö–æ–Ω—Ç–µ–∫—Å—Ç –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è
  nodeId        String?  // ID —É–∑–ª–∞, –¥–ª—è –∫–æ—Ç–æ—Ä–æ–≥–æ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–µ
  type          AISuggestionType
  
  // –°–æ–¥–µ—Ä–∂–∏–º–æ–µ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è
  title         String   // –ö—Ä–∞—Ç–∫–æ–µ –æ–ø–∏—Å–∞–Ω–∏–µ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è
  content       Json     // –ü–æ–¥—Ä–æ–±–Ω–æ–µ —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è
  confidence    Float    @default(0.5) // 0-1, —É–≤–µ—Ä–µ–Ω–Ω–æ—Å—Ç—å AI
  
  // –°—Ç–∞—Ç—É—Å –∏ —Ñ–∏–¥–±–µ–∫
  status        AISuggestionStatus @default(PENDING)
  userFeedback  String?  // –¢–µ–∫—Å—Ç–æ–≤—ã–π —Ñ–∏–¥–±–µ–∫ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
  rating        Int?     // 1-5 —Ä–µ–π—Ç–∏–Ω–≥ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è
  appliedAt     DateTime?
  
  // –°–≤—è–∑–∏
  request       AIRequest @relation(fields: [requestId], references: [id], onDelete: Cascade)
  user          User      @relation("UserAISuggestions", fields: [userId], references: [id], onDelete: Cascade)
  project       Project   @relation("ProjectAISuggestions", fields: [projectId], references: [id], onDelete: Cascade)
  
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  
  @@map("ai_suggestions")
  @@index([userId, status])
  @@index([projectId, nodeId])
  @@index([status, createdAt])
}

// –ú–æ–¥–µ–ª—å –¥–ª—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å—Å–∫–∏—Ö –Ω–∞—Å—Ç—Ä–æ–µ–∫ AI
model AIUserSettings {
  id                String   @id @default(auto()) @map("_id") @db.ObjectId
  userId            String   @unique @db.ObjectId
  
  // –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –ø—Ä–æ–∞–∫—Ç–∏–≤–Ω–æ—Å—Ç–∏
  proactiveMode     Boolean  @default(true)
  predictionRadius  Int      @default(1) // 1 –∏–ª–∏ 2 —à–∞–≥–∞
  suggestionDelay   Int      @default(3) // —Å–µ–∫—É–Ω–¥—ã
  
  // –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –ø—Ä–æ–≤–∞–π–¥–µ—Ä–æ–≤
  preferredProvider AIProvider @default(GEMINI)
  preferredModel    String   @default("gemini-2.5-flash") 
  creativityLevel   Float    @default(0.7) // 0-1
  
  // –¢–∏–ø—ã –∞–∫—Ç–∏–≤–Ω—ã—Ö –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–π
  activeTypes       AISuggestionType[] @default([STRUCTURE_ONLY, NEXT_NODES])
  
  // –û–±—É—á–µ–Ω–∏–µ
  learningEnabled   Boolean  @default(true)
  
  // –°–≤—è–∑–∏
  user              User     @relation("UserAISettings", fields: [userId], references: [id], onDelete: Cascade)
  
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  
  @@map("ai_user_settings")
}

// üí∞ –¢–û–õ–¨–ö–û –ë–ê–õ–ê–ù–° –ö–†–ï–î–ò–¢–û–í (SRP - –µ–¥–∏–Ω—Å—Ç–≤–µ–Ω–Ω–∞—è –æ—Ç–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ—Å—Ç—å)
model UserCredits {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  userId          String   @db.ObjectId
  teamId          String   @db.ObjectId  // ID –∫–æ–º–∞–Ω–¥—ã (–≤—Å–µ–≥–¥–∞ –µ—Å—Ç—å, –≤–∫–ª—é—á–∞—è –ø–µ—Ä—Å–æ–Ω–∞–ª—å–Ω—É—é)
  
  // –ë–∞–ª–∞–Ω—Å—ã –ø–æ —Ç–∏–ø–∞–º –∫—Ä–µ–¥–∏—Ç–æ–≤ (—Ç—Ä–∞—Ç—è—Ç—Å—è –≤ –ø–æ—Ä—è–¥–∫–µ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–∞)
  bonusCredits        Int      @default(500)    // –ë–æ–Ω—É—Å–Ω—ã–µ (–ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç 1, –Ω–µ —Å–≥–æ—Ä–∞—é—Ç) - –≤–∫–ª—é—á–∞–µ—Ç –±—ã–≤—à–∏–µ —Å—Ç–∞—Ä—Ç–æ–≤—ã–µ 500
  subscriptionCredits Int      @default(0)      // –ü–æ–¥–ø–∏—Å–æ—á–Ω—ã–µ (–ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç 2, —Å–≥–æ—Ä–∞—é—Ç)
  
  // –°–≤—è–∑–∏
  user            User                    @relation("UserCredits", fields: [userId], references: [id], onDelete: Cascade)
  team            Team                    @relation("TeamCredits", fields: [teamId], references: [id], onDelete: Cascade)
  transactions    CreditTransaction[]
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  @@unique([userId, teamId])
  @@map("user_credits")
  @@index([userId])
  @@index([teamId])
}

// üö¶ –õ–ò–ú–ò–¢–´ –ò –ü–û–õ–ò–¢–ò–ö–ò (SRP - –æ—Ç–¥–µ–ª—å–Ω–∞—è –æ—Ç–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ—Å—Ç—å)  
model CreditPolicy {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  userId          String   @db.ObjectId
  teamId          String?  @db.ObjectId
  
  // –õ–∏–º–∏—Ç—ã
  dailyLimit      Int      @default(10000)    // –î–Ω–µ–≤–Ω–æ–π –ª–∏–º–∏—Ç
  monthlyLimit    Int      @default(50000)    // –ú–µ—Å—è—á–Ω—ã–π –ª–∏–º–∏—Ç
  
  // –°–≤—è–∑–∏
  user            User     @relation("UserCreditPolicy", fields: [userId], references: [id], onDelete: Cascade)
  team            Team?    @relation("TeamCreditPolicy", fields: [teamId], references: [id], onDelete: SetNull)
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  @@unique([userId, teamId])
  @@map("credit_policies")
  @@index([userId])
  @@index([teamId])
}

// üìà –°–¢–ê–¢–ò–°–¢–ò–ö–ê –ò–°–ü–û–õ–¨–ó–û–í–ê–ù–ò–Ø (SRP - –æ—Ç–¥–µ–ª—å–Ω–∞—è –æ—Ç–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ—Å—Ç—å)
model UsageStats {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  userId          String   @db.ObjectId
  teamId          String?  @db.ObjectId
  period          UsagePeriod             // daily / monthly
  
  // –°—á–µ—Ç—á–∏–∫–∏
  used            Int      @default(0)    // –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–æ –≤ –ø–µ—Ä–∏–æ–¥–µ
  resetDate       DateTime                // –î–∞—Ç–∞ —Å–±—Ä–æ—Å–∞ —Å—á–µ—Ç—á–∏–∫–∞
  
  // –°–≤—è–∑–∏
  user            User     @relation("UserUsageStats", fields: [userId], references: [id], onDelete: Cascade)
  team            Team?    @relation("TeamUsageStats", fields: [teamId], references: [id], onDelete: SetNull)
  
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  @@unique([userId, teamId, period])
  @@map("usage_stats")
  @@index([userId, period])
  @@index([teamId, period])
}

// üìä –¢–†–ê–ù–ó–ê–ö–¶–ò–ò –ö–†–ï–î–ò–¢–û–í (–ø–µ—Ä–µ–∏–º–µ–Ω–æ–≤–∞–Ω–Ω–∞—è –∏ —É–ø—Ä–æ—â–µ–Ω–Ω–∞—è)
model CreditTransaction {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  creditsId   String   @db.ObjectId
  
  // –î–µ—Ç–∞–ª–∏ —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–∏
  type        CreditTransactionType
  creditType  CreditType           // –¢–∏–ø –∫—Ä–µ–¥–∏—Ç–æ–≤ (–±–æ–Ω—É—Å–Ω—ã–µ/—Å—Ç–∞—Ä—Ç–æ–≤—ã–µ/–ø–æ–¥–ø–∏—Å–æ—á–Ω—ã–µ)
  amount      Int                  // –ü–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω–æ–µ –¥–ª—è –ø–æ–ø–æ–ª–Ω–µ–Ω–∏—è, –æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω–æ–µ –¥–ª—è —Å–ø–∏—Å–∞–Ω–∏—è
  description String               // –û–ø–∏—Å–∞–Ω–∏–µ –æ–ø–µ—Ä–∞—Ü–∏–∏
  requestId   String?  @db.ObjectId // –°–≤—è–∑—å —Å AI –∑–∞–ø—Ä–æ—Å–æ–º, –µ—Å–ª–∏ –ø—Ä–∏–º–µ–Ω–∏–º–æ
  
  // Stripe –¥–∞–Ω–Ω—ã–µ (–¥–ª—è –ø–æ–∫—É–ø–æ–∫)
  stripePaymentIntentId String? // ID –ø–ª–∞—Ç–µ–∂–∞ –≤ Stripe
  stripeSubscriptionId  String? // ID –ø–æ–¥–ø–∏—Å–∫–∏ –≤ Stripe
  
  // –°–≤—è–∑–∏
  credits     UserCredits @relation(fields: [creditsId], references: [id], onDelete: Cascade)
  
  createdAt   DateTime @default(now())
  
  @@map("credit_transactions")
  @@index([creditsId, createdAt])
  @@index([stripePaymentIntentId])
  @@index([stripeSubscriptionId])
}

