  "type": "module",
  "main": "index.ts",
  "prisma": {
    "schema": "prisma/schema"
  },
  "scripts": {
    "dev": "NODE_ENV=development npx prisma generate && tsx watch src/server.ts",
    "dev:local": "NODE_ENV=development npx prisma generate && docker start postgres-dev && tsx watch src/server.ts",
    "build": "NODE_ENV=development npx prisma generate && tsc",
    "start": "NODE_ENV=production tsx src/server.ts",
    "migrate:dev": "npx dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:reset": "npx dotenv -e .env.development -- npx prisma migrate reset --force",
    "migrate:prod": "npx dotenv -e .env.production -- npx prisma migrate deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests"
  },
