{
  "name": "goflow",
  "version": "0.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development next dev --turbopack",
    "build": "prettier --check . && npm run stylelint:base && npm run lint:base && next build",
    "build:local": "prettier --write . && npm run stylelint:fix && npm run lint:base -- --fix && next build",
    "build:qa": "NODE_ENV=qa npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "start": "next start -H 0.0.0.0",
    "start:prod": "NODE_ENV=production next start -H 0.0.0.0",
    "lint:base": "eslint . --ext .js,.jsx,.ts,.tsx",
    "stylelint:base": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "lint": "npm run lint:base && npm run stylelint:base",
    "lint:fix": "npm run lint:base -- --fix && npm run stylelint:fix",
    "format": "prettier --write . && npm run stylelint:fix",
    "check:all": "npm run format && npm run lint:fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:engine": "jest \"src/tests/(unit|integration)/.*\\.(ts|tsx)$\" --verbose"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{css,scss}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.17.6",
    "@amplitude/engagement-browser": "^1.0.0",
    "@amplitude/plugin-session-replay-browser": "^1.16.13",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@types/dompurify": "^3.2.0",
    "@types/jszip": "^3.4.0",
    "@types/marked": "^5.0.2",
    "@types/socket.io-client": "^1.4.36",
    "@xyflow/react": "^12.4.4",
    "classnames": "^2.5.1",
    "d3-quadtree": "^3.0.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "dompurify": "^3.2.5",
    "framer-motion": "^12.5.0",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "immer": "^10.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.511.0",
    "marked": "^15.0.11",
    "next": "^15.3.0",
    "radix-ui": "^1.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.3",
    "reactflow": "^11.11.4",
    "recharts": "^3.1.2",
    "sass": "^1.86.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.22.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/d3-quadtree": "^3.0.6",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.16.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.3",
    "typescript": "^5.8.2"
  }
}
